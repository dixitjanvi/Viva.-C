//program on pointer...

#include<stdio.h>
#include<math.h>
//declaration of variable
int Add{int a,int b};
int Sub{int a,int b};
int Multi{int a,int b};
int Div{int a,int b};

int Add{int a,int b};
{
int c;
return a+b;
}

int Sub{int a,int b};
{
int c;
return a-b;
}

int Multi{int a,int b};{
int c;
return a*b;
}

int Div{int a,int b};{
int c;
return a/b;
}

int main()
{
printf("Enter the value of a & b");

Add(10,5);
Sub(10,5);
Multi(10,5);
Div(10,5);

return 0;
}
